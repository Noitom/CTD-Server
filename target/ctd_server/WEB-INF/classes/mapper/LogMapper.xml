<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.snf.dsds.dao.LogDao">
    <insert id="addLog">
        insert into t_log(username,request_url,request_param,request_date)values(#{username},#{requestUrl},#{requestParam},NOW())
    </insert>
</mapper>